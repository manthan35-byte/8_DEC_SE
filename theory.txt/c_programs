pro 23:
//example of nested if else
#include<stdio.h>
void main()
{
int num1,num2,num3;

printf("enter a number 1:");
scanf("%d",&num1);

printf("enter a number 2:");
scanf("%d",&num2);

printf("enter a number 3:");
scanf("%d",&num3);

if(num1>num2)
{
    if("num1>num3")
    {
        printf("\nnum1 is greater");
    }
    else 
    {
        printf("\nnum3 is greater");
    }

}
else
{
    if(num2>num3)
    {
        printf("\nnum2 is grater");
    }
    else
    {
        printf("\nnum3 is grater");
    }
}
}
