pro 17:
//find even and odd number
#include<stdio.h>
void main()
{
   int num;
   printf("enter a number");
   scanf("%d",&num);
  if(num>0)
  {
    printf("\n %d num is positive",num);  
  }
  else
  {
    printf("\n %d num is negetive",num);  
  }
}
