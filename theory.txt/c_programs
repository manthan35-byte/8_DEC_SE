pro 5:
#include <stdio.h>

void main()
{
  float price; // variable declration 
  
  printf("enter price :");
  scanf("%f",&price);
  
  printf("\n price = %f",price);
  
    

}

========================================================

#include <stdio.h>

void main()
{
  int num;
  
  
  printf("enter  num : ");
  scanf("%f",&num);
  
  printf("\n num = %f",num);
  

}
 
================================================================
void main()

{
    int num1;
    int num2;
    int num3;
   
    
    printf("enter a number :");
    scanf("%d",num1);
    
    printf("enter a number :");
    scanf("%d",num2);
    
    printf("enter a number :");
    scanf("%d",num3);
    
    
    
    printf(" \n %d,%d,%d",num1);
}


