pro 16:
//swap 2 number
#include<stdio.h>
void main()
{
    int a,b,c;
    a=10;
    b=20;
    
    printf("a=%d",a);
    printf("\nb=%d",b);
    
    c=a;
    a=b;
    b=c;
    printf("\na=%d",a);
    printf("\nb=%d",b);
}
