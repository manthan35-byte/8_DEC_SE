pro 5:

include<stdio.h>
void main()

{
    int num1;
    int num2;
    int num3;
   
    
    printf("enter a number :");
    scanf("%d",num1);
    
    printf("enter a number :");
    scanf("%d",num2);
    
    printf("enter a number :");
    scanf("%d",num3);
    
    
    
    printf(" \n %d,%d,%d",num1,num2,num3);
}

