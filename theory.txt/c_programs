pro 19:
operator presidensy:::
#include<stdio.h>
void main()
{
  int ans;

  ans=10+2*3;
  printf("\n ans=%d",ans);

  ans=10*5/2;
  printf("\n ans=%d",ans);

  #include<stdio.h>
void main()
{
  int ans;

  ans=10+2*3;
  printf("\n ans=%d",ans);

  ans=10+2+(30-3);
  printf("\n ans=%d",ans);

}

}
