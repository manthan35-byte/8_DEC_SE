pro 22:
#include<stdio.h>
void main()
{
int num1,num2;

printf("enter a number 1:");
scanf("%d",&num1);

printf("enter a number 2:");
scanf("%d",&num2);


if (num1>num2)
{
    printf("num1= %d is grater",num1);
}
else 
{
   printf("num1= %d is grater",num2);
}
}
